<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.contacts.groupcontactsmanager"
    android:versionCode="8"
    android:versionName="1.25" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/title"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.contacts.groupcontactsmanager.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>        
        <activity
            android:name="com.contacts.groupcontactsmanager.Send_1FormatPhoneEmail"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.contacts.groupcontactsmanager.Import_6_FormatToContacts"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.contacts.groupcontactsmanager.FilesList"
            android:label="@string/title_activity_files_list" >
        </activity>
        <activity
            android:name="com.contacts.groupcontactsmanager.Files"
            android:label="@string/title_activity_files" >
        </activity>
    </application>
</manifest>
